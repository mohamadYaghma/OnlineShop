{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {

      "@/*": ["./src/*"],
        "@/pages/*": ["src/pages/*"],
        "@/components/*": ["src/components/*"],
        "@/container/*": ["src/container/*"],
        "@/context/*": [
          "src/context/*"
        ],
        "@/hooks/*": [
          "src/hooks/*"
        ],
        "@/common/*": [
          "src/common/*"
        ],
        "@/utils/*": [
          "src/utils/*"
        ],
        "@/style/*": [
          "style/*"
        ],
        "@/*": [
          "./*"
        ],
        "@/services/*": ["src/services/*"]
    }
    
  
  }
}
  